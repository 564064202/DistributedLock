<?xml version="1.0"?>
<package >
  <metadata>
    <id>DistributedLock</id>
    <version>1.2.0</version>
    <title>DistributedLock</title>
    <authors>Michael Adelson</authors>
    <licenseUrl>https://raw.githubusercontent.com/madelson/DistributedLock/master/License.txt</licenseUrl>
    <projectUrl>https://github.com/madelson/DistributedLock</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>
      Provides easy-to-use and robust fully distributed locks (using SQLServer) as well as wrappers for named system-wide locks (using WaitHandles)
    </summary>
    <description>
      Provides easy-to-use and robust fully distributed locks (using SQLServer) as well as wrappers for named system-wide locks (using WaitHandles)
    </description>
    <releaseNotes>
      1.2.0 adds reader-writer locks, .NET Core support, new connection management options and IDbConnection/IDbTransaction support  
    </releaseNotes>
    <copyright>Copyright 2017 Michael Adelson</copyright>
    <tags>distributed lock async waithandle mutex sql sqlserver reader writer</tags>
    <dependencies>
	  <group targetFramework="net45"></group>
      <group targetFramework="netstandard1.3">
        <dependency id="NETStandard.Library" version="1.6.0"/>
        <dependency id="System.Data.SqlClient" version="4.3.0"/>
		<dependency id="System.Threading.AccessControl" version="4.3.0"/>
		<dependency id="System.Threading.ThreadPool" version="4.3.0"/>
      </group>
    </dependencies>
  </metadata>
  <files>
	<file src="DistributedLock\bin\Debug\DistributedLock.dll" target="lib\net45"/>
    <file src="DistributedLock\bin\Debug\DistributedLock.XML" target="lib\net45"/>
